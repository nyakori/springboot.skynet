    <select id="queryFeeDetails" resultMap="map">
        SELECT
        <include refid="item"/>
        FROM
            FIN_OPB_FEEDETAIL
        <where>
            <if test="clinicCode != null">
                AND CLINIC_CODE = #{clinicCode}
            </if>
            <if test="cardNo != null">
                AND CARD_NO = #{cardNo}
            </if>
            <if test="clinicCode == null and cardNo == null">
                AND CLINIC_CODE = #{clinicCode}
                AND CARD_NO = #{cardNo}
            </if>
            <if test="itemCode != null">
                AND ITEM_CODE = #{itemCode}
            </if>
            <if test="beginOperDate != null">
                AND OPER_DATE &gt;= #{beginOperDate} + 0
            </if>
            <if test="endOperDate != null">
                AND OPER_DATE &lt;= #{endOperDate} + 0
            </if>
        </where>
